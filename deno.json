{
  "nodeModulesDir": "manual",
  "tasks": {
    "prettier": "deno run -A npm:prettier@3.6.2 --check .",
    "prettier:write": "deno run -A npm:prettier@3.6.2 --write .",
    "oxlint": "deno run -A npm:oxlint@1.43.0 --deny-warnings .",
    "lint": "deno task prettier && deno task oxlint",
    "test": "deno test",
    "check": "deno task lint && deno check main.ts routes/*.tsx utils.ts static/app.js static/wasm_runtime.js tests/*.ts && deno task test",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "lint": {
    "rules": {
      "tags": ["fresh", "recommended"]
    }
  },
  "exclude": ["**/_fresh/*"],
  "imports": {
    "@/": "./",
    "fresh": "jsr:@fresh/core@^2.2.0",
    "preact": "npm:preact@^10.27.2",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.8",
    "vite": "npm:vite@^7.1.3"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.asynciterable", "dom.iterable", "deno.ns"],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": ["vite/client"]
  }
}
