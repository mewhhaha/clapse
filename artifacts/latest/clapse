#!/usr/bin/env bash
set -euo pipefail
SELF_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
export CLAPSE_COMPILER_WASM_PATH="${CLAPSE_COMPILER_WASM_PATH:-${SELF_DIR}/clapse_compiler.wasm}"
exec deno run -A "https://raw.githubusercontent.com/mewhhaha/clapse/v0.1.0.7/scripts/clapse.mjs" --wasm "$@"
